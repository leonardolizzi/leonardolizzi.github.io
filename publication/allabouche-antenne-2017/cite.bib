@inproceedings{allabouche_antenne_2017,
 abstract = {Dans ce travail, nous comparons deux différentes topologies d'antennes intégrées dans un deux différentes topologies d'antennes intégrées dans un dispositif IoT miniature fonctionnant de 900 MHz:Antenne F inversée (IFA) et antenne à Résonnateur Diélectrique (ARD). Ce type d'appareil portatif peut être fortement affecté par l'effet du corps humain, par conséquent nous étudions les performances des caractéristiques du rayonnement des deux structures à proximité d'une main humaine. Les stimulations électromagnétiques montrent pour l'IFA, une diminution de 15 dB de l'efficacité totale de de rayonnement en présence de la main, tandis que dans le cas de l'ARD, cette valeur est seulement diminuée de 2 dB dans la même situation.},
 address = {Saint-Malo, France},
 author = {Allabouche, Kaoutar and Ferrero, Fabien and Ribero, Jean-Marc and Lizzi, Leonardo and Jorio, Mohamed and Idrissi, Najiba El Amrani El},
 booktitle = {20èmes Journées Nationales Micro-Ondes (JNM 2017)},
 copyright = {All rights reserved},
 file = {Snapshot:/home/leolizzi/Zotero/storage/2GBSCXMY/fr.html:text/html},
 keywords = {natconf},
 language = {fr},
 month = {May},
 title = {Antenne à Résonateur Diélectrique pour réduire l’effet du corps humain dans un dispositif IoT miniature},
 url = {https://hal.archives-ouvertes.fr/hal-01519525},
