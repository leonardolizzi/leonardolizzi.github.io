@inproceedings{lizzi_antenne_2017,
 abstract = {Cet article décrit la conception d’une antenne F inversé (IFA) miniature et multi-bandes, pour l’intégration dans un terminal pour l’Internet des Objets (IoT). L’antenne fonctionne dans les bandes LoRa US (915 MHz), GPS L1 (1.57 GHz) et L2 (1.23 GHz). Les dimensions sont de 0.04 lambda ×0.08 lambda ×0.003 lambda à 915 MHz. Les mesures de l’antenne intégrée dans un terminal IoT, ainsi que des mesures de ce terminal dans un scénario réel sont reportées.},
 address = {Saint-Malo, France},
 author = {Lizzi, Leonardo and Ferrero, Fabien and Danchesi, Christophe and Boudaud, Stéphane},
