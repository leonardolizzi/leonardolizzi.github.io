 issn = {1536-1225},
 journal = {IEEE Antennas and Wireless Propagation Letters},
 keywords = {Antenna measurements, Dipole antennas, antenna radiation patterns, Antenna radiation patterns, Resonant frequency, omnidirectional antennas, cellular radio, GSM, omnidirectional radiation pattern, Wires, GSM band, dielectric substrate, classical half-wave dipole, foam material, foams, GSM center frequency, light, low-profile, low-profile GSM omnidirectional antenna, omnidirectional antenna},
