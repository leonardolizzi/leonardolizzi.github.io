 file = {IEEE Xplore Abstract Record:/home/leolizzi/Zotero/storage/YEMFUQV7/5172127.html:text/html;IEEE Xplore Full Text PDF:/home/leolizzi/Zotero/storage/AIBJKAYW/Lizzi et al. - 2009 - PSO-based time-domain antenna synthesis for enhanc.pdf:application/pdf},
 keywords = {antenna radiation patterns, Antenna radiation patterns, particle swarm optimisation, Bandwidth, ultra wideband communication, ultra wideband antennas, Transmitting antennas, Ultra wideband antennas, antenna geometry, Time domain analysis, Ultra wideband communication, Frequency, Antenna accessories, omnidirectional radiation pattern, PSO-based time-domain antenna synthesis, Reflector antennas, Signal analysis, time-domain synthesis, ultra wideband antenna, UWB communication system},
