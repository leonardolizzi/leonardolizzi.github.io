 keywords = {antenna radiation patterns, MIMO, Long Term Evolution, multifrequency antennas, antenna feeds, antenna isolation., decoupling radiating elements, LTE smart cells, monoband structure, Monopole antenna, monopole antenna arrays, multiband structure, multistandard multiantenna systems, mutual coupling, parasitic elements, phase shifter, phase shifters, power splitter, printed monopoles},
 month = {July},
