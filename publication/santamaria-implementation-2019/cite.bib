@inproceedings{santamaria_implementation_2019,
 abstract = {Dans cet article, nous analysons l'implémentation d'une antenne fente reconfigurable en diagramme. La reconfiguration repose sur l'utilisation d'interrupteurs électroniques qui contrôlent l'état de chaque fente (ouvert ou fermé). Différents mécanismes de commutation basés sur des diodes PIN et leur incidence sur les performances de l'antenne sont discutés. L'antenne implémentée fournit une couverture de 360° à travers six modes directifs et un mode omnidirectionnel.},
 address = {Caen, France},
 author = {Santamaria, Luca and Ferrero, Fabien and Staraj, Robert and Lizzi, Leonardo},
 booktitle = {Journées Nationales Micro-Ondes (JNM 2019)},
 copyright = {All rights reserved},
 file = {Snapshot:/home/leolizzi/Zotero/storage/AUYLUW7E/fr.html:text/html},
 keywords = {natconf},
 language = {fr},
 month = {May},
 title = {Implémentation d'une antenne fente reconfigurable en diagramme basée sur des diodes PIN de faible capacité},
 url = {https://hal.archives-ouvertes.fr/hal-02072357},
 urldate = {2020-05-04},
 year = {2019}
